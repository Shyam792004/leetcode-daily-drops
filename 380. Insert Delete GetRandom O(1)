class RandomizedSet {

     ArrayList<Integer>alist=null;
     HashMap<Integer,Integer> map=null;
     java.util.Random rand =new java.util.Random();
     
    public RandomizedSet() {
        alist=new ArrayList<>();
        map=new HashMap<>();
    }
    
    public boolean insert(int val) {
        if(map.containsKey(val))
        {
            return false;
        }
        else
        {
            map.put(val,alist.size());
            alist.add(val);
            return true;
        }
    }
    
    public boolean remove(int val) {
        if(map.containsKey(val))
        {
            int index=map.get(val);
            if(index<alist.size()-1)
            {
                //index will store the index_val of the element need to delete ----> from map 
                //last will store the last index value--->from list 
                int last=alist.get(alist.size()-1);
                alist.set(index,last);//replace the last  in the index position alist
                map.put(last,index);// update 6's index
            }
            alist.remove(alist.size()-1);
            map.remove(val);//remove the key(val) and its value(index);
            return true;
        }
        return false;
    }
    
    public int getRandom() {
        return alist.get(rand.nextInt(alist.size()));
    }
}

/**
 * Your RandomizedSet object will be instantiated and called as such:
 * RandomizedSet obj = new RandomizedSet();
 * boolean param_1 = obj.insert(val);
 * boolean param_2 = obj.remove(val);
 * int param_3 = obj.getRandom();
 */
